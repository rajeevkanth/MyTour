        <div id="car2" style="background:#f00;height:150px;width:150px;position:absolute;">
         <div id="car3" style="height:150px;width:150px;position:absolute;background-image:url(style/images/art/Brisbane3.jpg);">
         <div id="car4" style="background:#f00;height:150px;width:150px;position:absolute;">
         <div id="car5" style="height:150px;width:150px;position:absolute;background-image:url(style/images/art/GC1.jpg);">
   
     </div>
     </div>
   

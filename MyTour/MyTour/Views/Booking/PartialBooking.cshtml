<div id="slidingSection">
<script type="text/javascript">
    $(function () {

        var message = $('#msg');
        message.hide();
        $("#car1").delay(750).animate({
            left: '175px'
        }, "slow",
        function ()
        {

            $('#car2').animate({ left: '+175px' }, "slow",
                function () {

                    $('#car3').animate({ left: '+175px' }, "slow",
                        function () {

                            $('#car4').animate({ left: '+175px' }, "slow", function () {

                                $('#car5').animate({ left: '+175px' }, "slow", function () {

                                    $('#car').animate({ top: '+175px' }, "slow", function () {
                                        
                                        
                                        message.fadeIn(1000);

                                                    });

                                                });

                                            });

                                        });
                                         
                                    });
                                     });

                                });
          
        

    
    </script>

<div id="header">
    <hgroup class="title">
    <h2>Opening soon.</h2>

    <h4 id="msg">Get ready to set yourself free..</h4>

    </hgroup>
